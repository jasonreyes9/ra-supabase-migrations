    "scripts": {
        "build": "lerna run build",
        "build-demo": "cd packages/demo && yarn build",
        "run-demo": "cd packages/demo && yarn start",
        "test-unit": "lerna run test-unit",
        "watch": "lerna run --parallel watch",
